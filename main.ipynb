    "# Loading the scan data\n",
    "\n",
    "import itk\n",
    "import numpy as np\n",
    "\n",
    "scan_path = \"folder\"\n",
    "mhd_data = itk.imread(scan_path)\n",
    "# convert to numpy array\n",
    "scan = np.array(itk.GetArrayFromImage(mhd_data), dtype=np.float32)\n",
    "scan.clip(-1000, 1000, scan)  # clip to reasonable HU values\n",
    "\n"
